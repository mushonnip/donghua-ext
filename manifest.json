{
  "manifest_version": 2,
  "name": "AniChin Tracker",
  "version": "0.1.0",
  "description": "Favorite series and track completed episodes on anichin.moe.",
  "permissions": ["storage", "https://anichin.moe/*"],
  "content_scripts": [
    {
      "matches": ["https://anichin.moe/*"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    }
  ]
}
